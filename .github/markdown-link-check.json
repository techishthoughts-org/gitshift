{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/techishthoughts/GitPersona"
    },
    {
      "pattern": "^https://github.com/techishthoughts/GitPersona/issues"
    },
    {
      "pattern": "^https://github.com/techishthoughts/GitPersona/discussions"
    },
    {
      "pattern": "^https://github.com/techishthoughts/GitPersona/releases"
    },
    {
      "pattern": "^https://github.com/techishthoughts/GitPersona/wiki"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^#"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/techishthoughts/GitPersona/blob/main/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
